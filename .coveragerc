[run]
source = .
omit = 
    */tests/*
    */test_*.py
    conftest.py
    setup.py
    */venv/*
    */virtualenv/*
    */.env/*
    */config/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod